function draw_sky()
  n = sky_depth
  for i=0,n do
    if i % 3 == 0 then
      color = 8 
    else
      color = 1
    end
      line(0, i, 128, i, color)
      -- line(0, i, 128, i, 8)
  end

end


function build_scenery()
  mheight = 24
  buildings = 32
  b_width = 128 / buildings

  for i=0,buildings do
    start = i * b_width
    building_height = rnd(mheight) + rnd(5) + 2

    add(scenery, { 
      x1 = start,
      y1 = sky_depth - building_height,
      x2 = start + b_width,
      y2 = sky_depth,
      color = 1
    })
  end

  return scenery
end



