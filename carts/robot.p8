function add_ship()
  s = {
    moving = false,
    facing = "left", 
    sp=3, 
    x = 40, 
    y = 60, 
    facing_sprites = { 
      left  = { 3, 4 },
      right = { 19, 20 },
      up    = { 21, 22 },
      down  = { 5, 6 }
      } 
    }

  add(actors, s)

  return s
end

